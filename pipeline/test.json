							],
							"outputs": [
								{
									"referenceName": "ds_ecdc_files_csv",
									"type": "DatasetReference",
									"parameters": {
										"fileName_ds": {
											"value": "@item().fileName",
											"type": "Expression"
										}
									}
								}
							]
						},
						{
							"name": "set_var_item",
							"type": "SetVariable",
