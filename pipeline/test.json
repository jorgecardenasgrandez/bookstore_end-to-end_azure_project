							"onInactiveMarkAs": "Succeeded",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "var_item",
								"value": {
									"value": "@item().fileName",
									"type": "Expression"
								}
							}
						}
					]
				}
			}
		],
