					"dataset": {
						"referenceName": "ds_config_copydata",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "foreach_items_json",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "lookup_config_json",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('lookup_config_json').output.value",
						"type": "Expression"
